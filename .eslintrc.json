{
  "extends": ["airbnb"],
  "parser": "@babel/eslint-parser",
  "plugins": ["react"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": { "browser": true },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "linebreak-style": "off",
    "func-names": "off",
    "class-methods-use-this": "off",
    "semi": ["error", "always"]
  }
}
